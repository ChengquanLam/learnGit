<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <constant name="struts.enable.DynamicMethodInvocation" value="false" />
    <constant name="struts.devMode" value="true" />
<package name="reg" extends="struts-default">
	<action name="register_enter" class="reg.RegisterAction" method="register_enter">
		<result name="register_enter_page">/jsp/register.jsp</result>
	</action>
	<action name="register" class="reg.RegisterAction" method="execute">
		<result name="success">/jsp/success.jsp</result>
	</action>
		
</package>


   
</struts>
